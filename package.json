{
  "name": "b-syntax-highlighting",
  "displayName": "B Syntax highlighting",
  "description": "Syntax highlighting for the B language",
  "version": "0.3.0",
  "publisher": "baleiko",
  "author": {
    "name": "Valerii Baleiko",
    "email": "baleyko@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.36.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baleyko/vscode-b-syntax-highlighting.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "b",
    "blang",
    "b language",
    "b programming",
    "b coding",
    "b development"
  ],
  "icon": "images/b.png",
  "devDependencies": {
    "vsce": "^1.70.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "b",
        "aliases": [
          "B",
          "b"
        ],
        "extensions": [
          ".b"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "b",
        "scopeName": "source.b",
        "path": "./syntaxes/b.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "b",
        "path": "./snippets/b-blocks.json"
      }
    ]
  }
}
