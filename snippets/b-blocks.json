{
  "auto-statement": {
    "prefix": "auto",
    "body": "auto $1;$2",
    "description": "auto statement"
  },
  "extrn-statement": {
    "prefix": "extrn",
    "body": "extrn $1;$2",
    "description": "extrn statement"
  },
  "break-operator": {
    "prefix": "break",
    "body": "break;$1",
    "description": "break operator"
  },
  "if-block": {
    "prefix": "if",
    "body": "if ($1) {$2}$3",
    "description": "if block"
  },
  "else-if-block-section": {
    "prefix": "elseif",
    "body": " else if ($1) {$2}$3",
    "description": "else-if block section"
  },
  "else-block-section": {
    "prefix": "else",
    "body": " else {$1}$2",
    "description": "else block section"
  },
  "switch-block": {
    "prefix": "switch",
    "body": "switch ($1) {$2}$3",
    "description": "switch block"
  },
  "case-block-section": {
    "prefix": "case",
    "body": "case $1:$2break;$3",
    "description": "case block section"
  },
  "default-block-section": {
    "prefix": "default",
    "body": "default:$1",
    "description": "default block section"
  },
  "while-block": {
    "prefix": "while",
    "body": "while ($1) {$2}$3",
    "description": "while block"
  },
  "function-declaration": {
    "prefix": "function",
    "body": "$1($2) {$3}$4",
    "description": "function declaration"
  },
  "return-statement": {
    "prefix": "return",
    "body": "return ($1);",
    "description": "return statement"
  }
}
